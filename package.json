{
  "name": "log-send",
  "version": "0.1.0",
  "description": "日报收集器",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "npm run remove:pack && npm run build:win && npm run build:mac",
    "remove:pack": "rm -rf logSend*",
    "build:win": "electron-packager . logSend --platform=win32 --arch=x64 --out logSendWin --electron-version 12.0.4 --overwrite  --ignore=logSendWin* --ignore='(logSendWin*|node_modules/electron-localstorage/localConfig.json)'",
    "build:mac": "electron-packager . logSend --platform=darwin --arch=x64 --out logSendMac --electron-version 12.0.4 --overwrite --ignore='(logSendWin*|node_modules/electron-localstorage/localConfig.json)'"
  },
  "keywords": [
    "log",
    "log-send",
    "logSend",
    "electron"
  ],
  "author": "ChenDongdong",
  "license": "ISC",
  "devDependencies": {},
  "dependencies": {
    "auto-launch": "^5.0.5",
    "electron-localstorage": "^1.0.5",
    "nodemailer": "^6.5.0"
  }
}
