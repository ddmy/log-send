{
  "name": "log-send",
  "version": "0.1.2",
  "description": "日报收集器",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "npm run remove:pack && npm version patch && npm run build:win && npm run build:mac",
    "remove:pack": "rm -rf logSend*",
    "build:win": "electron-packager . logSend --platform=win32 --arch=x64 --icon=./img/logo/icon.ico --out logSendWin --electron-version 12.0.4 --overwrite  --ignore=logSendWin*",
    "build:mac": "electron-packager . logSend --platform=darwin --arch=x64 --icon=./img/logo/icon.icns --out logSendMac --electron-version 12.0.4 --overwrite --ignore=logSendWin*"
  },
  "keywords": [
    "log",
    "log-send",
    "logSend",
    "electron"
  ],
  "author": "ChenDongdong",
  "license": "ISC",
  "dependencies": {
    "auto-launch": "^5.0.5",
    "axios": "^0.21.1",
    "electron-localstorage": "^1.0.5",
    "nodemailer": "^6.5.0"
  }
}
